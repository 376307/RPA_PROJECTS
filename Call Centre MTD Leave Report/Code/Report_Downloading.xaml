                <ui:ExecuteQuery ContinueOnError="{x:Null}" TimeoutMS="{x:Null}" CommandType="Text" DataTable="[Output]" DisplayName="Run query" ExistingDbConnection="[RpaConn]" sap:VirtualizedContainerService.HintSize="334,88" sap2010:WorkflowViewState.IdRef="ExecuteQuery_1" Sql="------Call Centre MTD Leave Report--------&#xA;SELECT&#xA;    f.emp_code, f.emp_name ,d.designation,v.dep_name,&#xA;    sum((case when  ((t.m_time IS NULL OR t.m_time = '')&#xA;    OR (t.e_time IS NULL OR t.e_time = ''))  then 1 else 0 end ) ) MTD_leave_count&#xA;FROM&#xA;    mana0809.attend t&#xA;    INNER JOIN mana0809.employee_master f ON t.emp_code = f.emp_code&#xA;    INNER JOIN mana0809.department_mst v ON v.dep_id = f.department_id&#xA;    INNER JOIN mana0809.designation_master d ON d.designation_id=f.designation_id&#xA;WHERE&#xA;     t.curr_date &gt;= trunc(sysdate,'mm')&#xA;    And t.curr_date&lt;=(sysdate-1)&#xA;AND v.dep_id = '633'&#xA;    AND f.status_id = '1'&#xA;GROUP BY&#xA;   f.emp_name, f.emp_code,v.dep_name,d.designation&#xA;ORDER BY&#xA;    MTD_leave_count DESC">
                  <ui:ExecuteQuery.Parameters>
